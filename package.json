{
  "name": "@untra/naiveasync",
  "version": "0.1.7",
  "description": "An Opinionated and Painless React promise wrapper",
  "main": "./dist/src/naiveasync/index.js",
  "types": "./dist/src/naiveasync/index.d.ts",
  "author": {
    "name": "Samuel Volin",
    "email": "sam@untra.io",
    "url": "https://github.com/untra"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/untra/naiveasync"
  },
  "scripts": {
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run ci && npm run defs",
    "postpublish": "npm run deploy",
    "preversion": "npm run ci",
    "test": "mocha --reporter spec",
    "build": "rm -f dist/index.html && cp src/index.html dist/index.html && tsc -p ./build.tsconfig.json",
    "build:watch": "npm run build && tsc -p ./tsconfig.json --watch",
    "build:site": "./node_modules/.bin/react-scripts build",
    "lint": "tslint --project \"./tsconfig.json\"",
    "docs": "typedoc --out docs/docs src",
    "defs": "tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "ci": "npm run lint && npm run build",
    "ci2": "npm run lint && npm run build && npm run cover",
    "start": "npm run build && ./node_modules/.bin/react-scripts start",
    "deploy": "./node_modules/.bin/gh-pages -d ./build",
    "predeploy": "npm run build:site",
    "watch": "./node_modules/.bin/webpack-dev-server dist/src/index.js",
    "serve:watch": "./node_modules/.bin/webpack --config webpack.config.js --mode development",
    "cypress:test": "./node_modules/.bin/cypress run --record ",
    "cypress:run": "./node_modules/.bin/cypress run",
    "cypress:open": "./node_modules/.bin/cypress open",
    "build:test": "./node_modules/.bin/webpack --output-filename out.js --entry ./cypress/integration/test.spec.ts"
  },
  "license": "MIT",
  "dependencies": {
    "react": "^16.12.0",
    "redux": "^4.0.5",
    "rxjs": "6.5.3",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.7",
    "@cypress/browserify-preprocessor": "^2.1.0",
    "@cypress/webpack-preprocessor": "^4.1.1",
    "@types/chai": "^4.2.7",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.17.13",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-highlight": "0.12.1",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "4.3.3",
    "@types/uuid": "^3.4.6",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "css-loader": "^3.4.0",
    "cypress": "^3.8.1",
    "eslint-plugin-react-hooks": "^1.6.1",
    "gh-pages": "^2.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "react-dom": "^16.12.0",
    "react-highlight": "0.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "5.0.1",
    "react-scripts": "^3.3.0",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
